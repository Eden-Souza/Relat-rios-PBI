	partition ANO-SAFRA = calculated
		mode: import
		source = ```
				
				VAR StartDate = DATE(2025, 3, 31)  -- Segunda-feira da Semana 1
				VAR NumWeeks = 39
				RETURN
				ADDCOLUMNS (
				    GENERATESERIES(1, NumWeeks, 1),
				    "Semana", [Value],
				    "Data Início", StartDate + ([Value] - 1) * 7,
				    "Data Fim", StartDate + ([Value] - 1) * 7 + 6,
				    "Semana Atual", "2025-" & FORMAT([Value], "00")
				)
				
				```

